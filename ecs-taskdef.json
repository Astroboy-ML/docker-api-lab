{
  "family": "docker-api-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::424051098783:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::424051098783:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "docker-api-container",
      "image": "424051098783.dkr.ecr.eu-west-3.amazonaws.com/docker-api-lab:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "eu-west-3",
          "awslogs-group": "/ecs/docker-api-task",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
